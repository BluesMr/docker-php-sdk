{
    "name": "blues-mr/docker-php-sdk",
    "description": "Docker Engine API v1.45 PHP SDK Client",
    "type": "library",
    "license": "MIT",
    "keywords": ["docker", "api", "sdk", "client", "engine"],
    "require": {
        "php": ">=8.1",
        "guzzlehttp/guzzle": "^7.0",
        "psr/http-message": "^1.0",
        "psr/log": "^3.0",
        "symfony/serializer": "^6.0",
        "symfony/property-access": "^6.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.0",
        "phpstan/phpstan": "^1.0",
        "squizlabs/php_codesniffer": "^3.0",
        "mockery/mockery": "^1.5"
    },
    "autoload": {
        "psr-4": {
            "Docker\\API\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Docker\\API\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "phpunit",
        "phpstan": "phpstan analyse",
        "cs-check": "phpcs",
        "cs-fix": "phpcbf"
    }

}



